        "glob": "^7.1.6",
        "jquery": "^3.4.1",
        "restify": "^8.5.1",
        "restify-router": "^0.5.1",
        "webpack-dev-server": "^3.9.0"
    },
    "browser": {
        "child_process": false
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ]
    }
}
